 /* product_navigation */
.nav_active{
 background:#f27d33;
 color:white;
}
.product_fap  .product_fap_list ul li .question .minus{
  display:none;
}
.product_navigation{
max-width:1448px;
margin:100px auto 30px;
padding:0 24px;
}
.fixed_top{
position: fixed;
  top: 0;
  z-index:9;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 0;
}
.product_navigation ul{
display:flex;
justify-content:space-between;
background:#f6f6f6;
margin:0;
}
.product_navigation ul li{
cursor:pointer;
list-style:none;
padding:1vw 3.1vw;
font-family: Poppins;
font-size: 20px;
font-style: normal;
font-weight: 400;
line-height: normal;
text-align:center;
}
.product_bg1 .bg{
  background-image:url('https://cdn.shopify.com/s/files/1/0805/5170/6923/files/product_bg1.png?v=1691391349');
  background-repeat:no-repeat;
  background-size: cover;
  height:47vw;
  overflow:hidden;
}
.product_bg1 {
  position:relative;
}
.product_bg1 .text{
  position:absolute;
  left: 50%;
  transform: translateX(-50%);
}
.product_bg1 .text .title{
color: #363636;
text-align: center;
font-family: Poppins;
margin-top:57px;
font-size: 36px;
font-style: normal;
font-weight: 700;
line-height: 70px;  
}
.product_bg1 .text .content{
color: #666;
margin-top:17px;
text-align: center;
font-family: Poppins;
font-size: 18px;
font-style: normal;
font-weight: 400;
line-height: 35px;  
}
.product_style{
max-width: 1448px;
padding: 24px;
margin: 0 auto;}

.product_style .title{
color: #363636;
text-align: center;
font-family: Poppins;
margin-top:57px;
font-size: 36px;
font-style: normal;
font-weight: 700;
line-height: 70px;  
}
.product_style .content{
color: #666;
margin-top:17px;
text-align: center;
font-family: Poppins;
font-size: 18px;
font-style: normal;
font-weight: 400;
line-height: 35px;  
}
.product_style .product_style_img{
text-align:center;
margin:20px auto 0;
}

.product_style .product_style_img img:nth-child(1){
margin-right:22px;
}

.product_icon_wrapper .product_icon {
width:80%;
display:flex;
justify-content:space-between;
margin: 0 auto;
}
.product_icon_wrapper .product_icon .product_icon_item{
text-align:center;
}

.img_wall{
margin:44px 0;
}
.img_wall .img_wall_list{
    display: flex;
}
.img_wall .list_bottom{
    flex-direction: row-reverse;
}
.img_wall  .img_wall_list .item{
width:50%;
}

.img_wall .img_wall_list .img_wall_item img{
display:block;}

.img_wall .img_wall_list .text{
padding: 10vw 0  0 2.7vw;
}
.img_wall .img_wall_list .text .title{
font-family: Poppins;
font-size: 36px;
font-weight: 700;
line-height: 70px;
letter-spacing: 0em;
text-align: left;  
}
.img_wall .img_wall_list .text .content{
font-family: Poppins;
font-size: 18px;
font-weight: 400;
line-height: 35px;
letter-spacing: 0em;
text-align: left;
}
.img_list{
margin: 95px auto 80px;
max-width:1448px;
padding:0 24px;
display:flex;
justify-content:space-between; 
}
.img_list .img_list_item img{
padding:1vw;
}
.img_list .img_list_item .title{
color: #363636;
font-family: Poppins;
font-size: 1vw;
font-style: normal;
font-weight: 400;
line-height: 70px; 
padding:0 28px;
}
.img_list .img_list_item .content{
padding:0 28px;
color: #666;
font-family: Poppins;
font-size: 0.85vw;
font-style: normal;
font-weight: 400;
line-height: 35px; 
}
.specifications{
margin: 0 auto 80px;
max-width: 1448px;
padding: 0 24px;
}
.specifications table{
font-family: Poppins;
font-size: 0.84vw;
font-weight: 400;
line-height: 35px;
letter-spacing: 0em;
text-align: left;
}
.specifications table tr:first-child th:after,.specifications table tr:first-child td:after{
border:none;
}
.specifications .table table tbody tr:nth-of-type(odd){
background: white;
}
.specifications .table table tbody tr td:nth-child(1){
padding-left:68px 
}
.specifications .table table tbody tr td:nth-child(2){
text-align: right;
padding-right: 68px;
}
.generic_title{
margin-bottom:4vw;
text-align:center;
}
.generic_title .title_text {
display:inline-block;
margin:0 auto;
font-family: Poppins;
font-size: 36px;
font-weight: 700;
line-height: 54px;
letter-spacing: 0em;
text-align: center;
position:relative;
}
.generic_title .title_text svg{
position:absolute;
left:100%;
top:0;
}
.product_reviews {
margin: 0 auto 80px;
max-width: 1448px;
padding: 0 24px;}
.product_reviews .reviews_list{
display:flex;
justify-content:space-between;
}
.product_fap{
margin: 0 auto 80px;
max-width: 1448px;
padding: 0 24px;
}
.product_fap  .product_fap_list ul li{
padding:20px 90px;
list-style:none;
border-bottom:1px solid #E9E9E9;
}
.product_fap  .product_fap_list ul li p{
display:none;
}
.product_fap  .product_fap_list ul li .question{
position:relative;
color: #363636;
font-family: Poppins;
font-size: 1vw;
font-style: normal;
font-weight: 400;
padding:20px 0;
line-height: 70px;  
}
.product_fap  .product_fap_list ul li .question svg{
position: absolute;
top: 24%;
left: -6%;
}
.product_fap  .product_fap_list ul li p{
color: #666;
font-family: Poppins;
font-size: 0.84vw;
font-style: normal;
padding:0.5vw 0;
font-weight: 400;
line-height: 30px; 
}
.product_download{
margin: 0 auto 100px;
max-width: 1448px;
padding: 0 24px;
}
.product_download .link{
margin-top:90px;}
.product_download .link p{
display: inline-block;
}
.product_download .link a{
position:relative;
margin-right:40px;
padding: 20px 165px 20px 55px;
color: #363636;
font-family: Poppins;
border-radius: 6px;
border: 1px solid #E9E9E9;
font-size: 20px;
font-style: normal;
font-weight: 400;
line-height: 70px; /* 350% */
}
.product_download .link a::before{
content: '';
  display: block; 
position:absolute;
background-image:url('https://cdn.shopify.com/s/files/1/0805/5170/6923/files/google-docs_1.png?v=1691473440');
background-repeat:no-repeat;
background-size: cover;
left: 3%;
top: 32%;
width: 24px;
height: 24px;
}
.product_download .link a::after{
content: '';
  display: block; 
position:absolute;
background-image:url('https://cdn.shopify.com/s/files/1/0805/5170/6923/files/download_1.png?v=1691473441');
background-repeat:no-repeat;
background-size: cover;
right: 3%;
top: 32%;
width: 24px;
height: 24px;
}
@media (max-width:1000px){
.product_icon_wrapper .product_icon{
  width:100%;
}
  .img_wall .img_wall_list .text {
    padding: 2vw 0 0 2.7vw;
}
.product_icon_wrapper .product_icon .product_icon_item{
  width:100%;
  } 
  .img_list .img_list_item img{
   padding:1vw;
  }
  .product_download .link a {
  display:block;
  margin-bottom:4vw;
}
}
@media (max-width:500px){
.product_fap .product_fap_list ul li .question svg{
left:-14%;
}
.product_download .link p{
width:100%;
}
.product_bg1 .text{
 position:unset;
 transform:unset;
 margin-bottom: 6vw;
}
.product_bg1 .text .title{
  font-size:5.8vw;
}
.generic_title .title_text{
    font-size: 5.3vw;
}
.product_style .title{
  font-size:5.8vw;
}
.product_style .product_style_img img{
  width:45%; 
}
.product_icon_wrapper .product_icon {
  width:100%;
  flex-wrap:wrap;
}
.product_icon_wrapper .product_icon .product_icon_item{
  width:50%;
}
.img_wall .img_wall_list{
  flex-wrap: wrap;
  justify-content: space-around;
}
.img_wall  .img_wall_list .item{
 width:90%;
 padding: 4vw 0;
}
.img_wall .img_wall_list .text .title{
  text-align:center;
font-size:5.3vw;
}
.img_list{
display:none;
}
.specifications .table table tbody tr td:nth-child(1){
padding-left:0 
}
.specifications .table table tbody tr td:nth-child(2){
text-align: right;
padding-right: 0;
}
.specifications .table  figure{
margin:0; 
}
.product_fap  .product_fap_list ul{
margin:0;
}
.product_fap  .product_fap_list ul li {
padding:2vw 0 2vw 9vw;
}
.product_fap .product_fap_list ul li .question{
font-size:4.8vw;
line-height:unset;
padding:0;
padding-left:3vw;
}
.product_fap .product_fap_list ul li::before {
width: 8.2vw;
height: 8.2vw;
font-size: 9vw;
line-height: 9vw;
border-radius: 5.5vw;
}
.product_download .link a{
display:block;
margin:0 0 4.8vw 0;
padding:0 0 0 12vw;
}
.specifications .table figure{
margin:0; 
}
}


